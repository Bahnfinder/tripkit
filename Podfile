# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'TripKit' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  platform :ios, '10.0'

  # Pods for TripKit
  pod 'GzipSwift', '~> 5.0'
  pod 'SWXMLHash', '~> 4.9.0'
  pod 'SwiftyJSON', '~> 4.0'

  target 'TripKitTests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'TripKit watchOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  platform :watchos, '3.0'

  # Pods for TripKit watchOS
  pod 'GzipSwift', '~> 5.0'
  pod 'SWXMLHash', '~> 4.9.0'
  pod 'SwiftyJSON', '~> 4.0'
end
